# IORA MCP Server Environment Configuration
# Copy this file to .env and configure your values

# Server Configuration
NODE_ENV=development
PORT=7070
LOG_LEVEL=info

# Coral Protocol Integration
CORAL_REGISTRY_AUTO_REGISTER=true
CORAL_REGISTRY_URL=https://registry.coralprotocol.org
CORAL_REGISTRY_TOKEN=your_registry_token_here
CORAL_REGISTRY_HEARTBEAT_INTERVAL=60
CORAL_REGISTRY_TIMEOUT=5000
CORAL_SERVER_URL=http://localhost:7070

# IORA Binary Path
IORA_BIN=../target/release/iora

# AI Provider Configuration
GEMINI_API_KEY=your_gemini_api_key
MISTRAL_API_KEY=your_mistral_api_key
AIMLAPI_KEY=your_aimlapi_key
LLM_PROVIDER=gemini
MISTRAL_MODEL=mistral-medium

# Cryptocurrency Data APIs
COINGECKO_API_KEY=your_coingecko_api_key
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key
CRYPTOCOMPARE_API_KEY=your_cryptocompare_api_key
COINPAPRIKA_API_KEY=your_coinpaprika_api_key

# Blockchain Configuration
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_WALLET_PATH=./wallets/devnet-wallet.json

# NFT Receipt Configuration (Crossmint)
CROSSMINT_PROJECT_ID=your_crossmint_project_id
CROSSMINT_SERVER_SECRET=your_crossmint_server_secret
CROSSMINT_CLIENT_KEY=your_crossmint_client_key
CROSSMINT_ENV=staging
CROSSMINT_RECIPIENT=your_solana_wallet_address

# Vector Database Configuration
TYPESENSE_API_KEY=your_typesense_api_key
TYPESENSE_URL=http://localhost:8108

# Security
CORAL_SHARED_SECRET=your_secure_random_secret_here
